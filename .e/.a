alias a=". \$ACTIVE_ENV_PATH/.a;m"
alias e=". \$ACTIVE_ENV_PATH/.e"
alias m=". \$ACTIVE_ENV_PATH/.m"
alias va="vi \$ACTIVE_ENV_PATH/.a;a"
alias ve="vi \$ACTIVE_ENV_PATH/.e;e"
alias vm="vi \$ACTIVE_ENV_PATH/.m;m"
alias killport=". \$ACTIVE_SCRIPTS_PATH/killport.sh"
alias kp3000="killport 3000"

alias runfe="cdfe; clear; clear; npm run dev 2>&1 | tee run.out"
alias rundev="runfe"
alias runprod="npm run build && npm start"

# NPM SETUP
alias npmclean="clear; cdfe; rm -rf node_modules package-lock.json .next"
alias npmi="npmclean; npm install @sponsorcoin/spcoin-access-modules-cjs"

alias rt="clear; . ./scripts/installNodeModules.sh 0"
alias vt="vi ./scripts/installNodeModules.sh"
alias is=". ./scripts/installNewScripts.sh"
alias fs="clear;. ./scripts/findInstallScripts.sh | tee fs.log"
alias vfs="vi ./scripts/findInstallScripts.sh"
alias home="cd  ~/spCoin/JUNK2/SPCOIN-PROJECT-MODULES"

alias t="clear; . ./arrsort.sh | tee arrsort.log"
alias vta="vi ./arrsort.sh"
alias rta=". ./arrsort.sh"
